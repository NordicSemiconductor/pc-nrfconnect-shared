{
  "name": "pc-nrfconnect-shared",
  "version": "4.8.15",
  "description": "Shared commodities for developing pc-nrfconnect-* packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/pc-nrfconnect-shared.git"
  },
  "author": "Nordic Semiconductor ASA",
  "license": "ISC",
  "bin": {
    "nrfconnect-scripts": "./bin/nrfconnect-scripts.js"
  },
  "main": "src",
  "scripts": {
    "lint": "node ./bin/nrfconnect-scripts.js lint src",
    "lint-init": "cp ./config/eslintrc.json .eslintrc",
    "test": "jest --testResultsProcessor jest-bamboo-formatter src",
    "test-watch": "jest --watch src"
  },
  "dependencies": {
    "@babel/core": "7.10.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.10.0",
    "@babel/plugin-transform-destructuring": "7.10.0",
    "@babel/plugin-transform-modules-commonjs": "7.9.6",
    "@babel/plugin-transform-parameters": "7.9.5",
    "@babel/plugin-transform-spread": "7.10.0",
    "@babel/preset-react": "7.10.0",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "commander": "5.1.0",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "file-loader": "6.0.0",
    "ftp": "0.3.10",
    "husky": "4.2.5",
    "immutable": "4.0.0-rc.12",
    "jest": "26.0.1",
    "jest-bamboo-formatter": "1.0.1",
    "moment": "2.26.0",
    "mousetrap": "1.6.5",
    "prettysize": "2.0.0",
    "react-bootstrap": "1.0.1",
    "react-flip-toolkit": "7.0.12",
    "react-ga": "2.7.0",
    "react-markdown": "4.3.1",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "rimraf": "3.0.2",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "semver": "7.3.2",
    "shasum": "1.0.2",
    "style-loader": "1.2.1",
    "systeminformation": "4.26.4",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0"
  },
  "peerDependencies": {
    "react": "^16.13",
    "react-redux": "^7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/mocks/emptyMock.js",
      "^electron$": "<rootDir>/mocks/electronMock.js",
      "^electron-store$": "<rootDir>/mocks/electronStoreMock.js",
      "pc-nrfjprog-js|nrf-device-lister|nrf-device-setup": "<rootDir>/mocks/emptyMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./config/babel.config.js"
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ]
  }
}
